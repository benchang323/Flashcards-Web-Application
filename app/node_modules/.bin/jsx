#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/benjamin/Desktop/Full-Stack Javascript/homework-7-2-flashcards-benchang323/node_modules/.pnpm/jsx@0.9.89/node_modules/jsx/bin/node_modules:/Users/benjamin/Desktop/Full-Stack Javascript/homework-7-2-flashcards-benchang323/node_modules/.pnpm/jsx@0.9.89/node_modules/jsx/node_modules:/Users/benjamin/Desktop/Full-Stack Javascript/homework-7-2-flashcards-benchang323/node_modules/.pnpm/jsx@0.9.89/node_modules:/Users/benjamin/Desktop/Full-Stack Javascript/homework-7-2-flashcards-benchang323/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/benjamin/Desktop/Full-Stack Javascript/homework-7-2-flashcards-benchang323/node_modules/.pnpm/jsx@0.9.89/node_modules/jsx/bin/node_modules:/Users/benjamin/Desktop/Full-Stack Javascript/homework-7-2-flashcards-benchang323/node_modules/.pnpm/jsx@0.9.89/node_modules/jsx/node_modules:/Users/benjamin/Desktop/Full-Stack Javascript/homework-7-2-flashcards-benchang323/node_modules/.pnpm/jsx@0.9.89/node_modules:/Users/benjamin/Desktop/Full-Stack Javascript/homework-7-2-flashcards-benchang323/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jsx/bin/jsx" "$@"
else
  exec node  "$basedir/../jsx/bin/jsx" "$@"
fi
